  AreaKecamatan     AreaKecamatan? @relation(fields: [idKecamatan], references: [id])
  idKecamatan       Int?
  AreaKelurahan     AreaKelurahan? @relation(fields: [idKelurahan], references: [id])
  idKelurahan       Int?

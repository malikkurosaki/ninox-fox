  infrastruktur     Int
  layananKesehatan  Int
  kemiskinan        Int
  lapanganPekerjaan Int
  keadilanSosial    Int
  isActive          Boolean        @default(true)
  createdAt         DateTime       @default(now())
  updatedAt         DateTime       @updatedAt
